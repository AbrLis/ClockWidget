<Application x:Class="ClockWidgetApp.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d">
    <Application.Resources>
        <ResourceDictionary>
            <BitmapImage x:Key="AppIcon" UriSource="/Resources/Icons/app.ico"/>
            <BitmapImage x:Key="AlarmIcon" UriSource="/Resources/Icons/alarm.ico"/>
            <BitmapImage x:Key="LongIcon" UriSource="/Resources/Icons/long.ico"/>
            <BitmapImage x:Key="TimerIcon" UriSource="/Resources/Icons/timer.ico"/>
        </ResourceDictionary>
    </Application.Resources>
</Application>
